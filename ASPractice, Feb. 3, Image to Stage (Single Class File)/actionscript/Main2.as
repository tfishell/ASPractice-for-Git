package actionscript {	import flash.display.MovieClip;	//import will automatically be added if you select the class from the dropdown list	public class Main2 extends MovieClip	{		public function Main2() {			// constructor code			for (var i:int=0; i<100; i++) {				var rock:Block = new Block();					rock.x = 200 + 2*i;					rock.y = 150 + 2*i;					addChild(rock);				}							}					}	}	